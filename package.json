{
  "name": "sprut",
  "version": "0.0.3",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "dirold2",
    "email": "i@dirod2.ru"
  },
  "imports": {
    "#*": [
        "./src/*",
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*.js",
        "./src/*.jsx",
        "./src/*/index.ts",
        "./src/*/index.tsx",
        "./src/*/index.js",
        "./src/*/index.jsx"
    ]
  },
  "description": "RAGU2 is a music bot for Discord that allows users to play, pause, and manage music tracks in voice channels. The bot supports integration with Yandex Music and provides various commands for controlling playback.",
  "homepage": "https://github.com/dirold2/sprut",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirold2/sprut.git"
  },
  "bugs": {
    "url": "https://github.com/dirold2/sprut/issues"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "yandex",
    "music"
  ],
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "node scripts/postbuild.js",
    "dev": "node --enable-source-maps --experimental-specifier-resolution=node --no-warnings --loader ts-node/esm/transpile-only src/main.ts",
    "start": "node --enable-source-maps --trace-warnings --experimental-specifier-resolution=node build/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write --use-tabs --tab-width 2 \"src/**/*.ts\"",
    "test": "test",
    "prisma:generate": "pnpx prisma generate --schema=./src/modules/database/prisma/schema.prisma",
    "pm2:start": "pm2 start ecosystem.config.json",
    "pm2:restart": "pm2 restart sprut",
    "pm2:stop": "pm2 stop sprut",
    "pm2:delete": "pm2 delete sprut",
    "pm2:list": "pm2 list"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "ansi-styles": "^6.2.1",
    "async-retry": "^1.3.3",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "delay": "6.0.0",
    "dirname-filename-esm": "^1.1.2",
    "dotenv": "^16.4.7",
    "ffmpeg-ffprobe-static": "6.1.2-rc.1",
    "https-proxy-agent": "^7.0.6",
    "install": "^0.13.0",
    "p-timeout": "6.1.4",
    "prisma": "6.5.0",
    "toucan-js": "4.1.1",
    "tslib": "^2.8.1",
    "typescript": "5.8.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/async-retry": "^1.4.9",
    "@types/node": "^22.14.0",
    "chokidar": "^4.0.3",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.29.0",
    "axios": "^1.8.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "workspaces": [
    "src/modules/*"
  ],
  "peerDependencies": {
    "typescript": "^5"
  }
}